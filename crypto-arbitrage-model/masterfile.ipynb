    "def feature_selection_2(df, exchange1, exchange2):\n",
    "    df_train = df.select([\n",
    "        pl.col([\"ts\", \"Bybit_delta_ts\", 'HTX_delta_ts', 'timedelta', 'arbitrage_id', \"arb_opportunity\", 'length', 'till_arbitrage', 'since_last_arbitrage', 'arbitrage_start', 'active_arbitrage']),\n",
